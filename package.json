  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "markdownlint-cli": "^0.32.2",
    "nodemon": "^3.0.1",
    "eslint": "^8.47.0",
    "eslint-config-codfish": "^11.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "lint-staged": "^13.1.0",
    "@commitlint/cli": "^17.6.7",
